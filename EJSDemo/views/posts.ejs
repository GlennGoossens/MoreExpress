<% include partials/header %>
<h1> The post page</h1>

<% for(var i = 0; i < posts.length; i++){%>
  <li>
    <%= posts[i].title %> - <strong><%= posts[i].author %></strong>
  </li>
<%}%>

<h2>with foreach</h2>
<% posts.forEach(function(post){%>
  <li>
  <%= post.title %> - <%= post.author %>
  </li>
<%})%>

<% include partials/footer %>
